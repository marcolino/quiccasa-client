{
  "name": "client",
  "version": "0.1.12",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "add": "^2.0.6",
    "aws-amplify": "^4.1.1",
    "bootstrap": "^5.0.2",
    "node-sass": "5",
    "prettier": "^2.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.0.1",
    "yarn": "^1.22.10"
  },
  "scripts": {
    "start": "react-scripts start",
    "start-production": "yarn build && serve -s build",
    "prebuild": "yarn install-pre-push-hook; yarn sitemap-build",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test-once": "react-scripts test --watchAll=false",
    "NO-env-crypt": "gpg -c --no-symkey-cache .env",
    "env-decrypt": "gpg -d .env.gpg",
    "sync": "aws s3 sync build/ s3://quiccasa.sistemisolari.com",
    "eject": "echo -n 'Really sure to eject [yes/no] ? '; read answer; [ $answer = 'yes' ] && react-scripts eject || true",
    "install-pre-push-hook": "cp scripts/git-hooks/pre-push .git/hooks/pre-push; chmod 700 .git/hooks/pre-push",
    "sitemap-build": "babel-node scripts/sitemap-build.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/node": "^7.14.7",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "@babel/preset-react": "^7.14.5",
    "@babel/register": "^7.14.5",
    "react-router-sitemap": "^1.2.0"
  }
}
